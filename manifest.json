{
  "manifest_version": 3,
  "name": "Random URL Saver",
  "version": "1.0.5",
  "description": "Save URLs and navigate to a random one later.",
  "permissions": ["storage", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "options_page": "settings.html"
}
